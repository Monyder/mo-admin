<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mon.sof.project.legalDate.mapper.LegalDateMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="mon.sof.project.legalDate.entity.LegalDate">
        <id column="id" property="id" />
        <result column="date" property="date" />
        <result column="legal_name" property="legalName" />
    </resultMap>

    <select id="findByCondition" parameterType="map" resultMap="BaseResultMap">
        <![CDATA[
		SELECT * FROM legal_date
		]]>
        <include refid="com.sof.mapper.base.conditionSql"/>
    </select>
</mapper>
